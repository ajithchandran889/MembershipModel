<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - one Konnect</title>
    <link href="~/Content/Style.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap-theme.css.map" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.css.map" rel="stylesheet" type="text/css" />
    <link href="~/Content/sticky-footer.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/navbar.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <header>
        <nav role="navigation" class="navbar navbar-default">
            <div class="container">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button data-target=".navbar-ex1-collapse" data-toggle="collapse" class="navbar-toggle" type="button">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="index.html" class="navbar-brand"><img src="~/Content/images/site/logo.png"></a>
                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse navbar-ex1-collapse">
                    <ul class="nav navbar-nav">
                        <li><a href="#">Home</a></li>
                        <li><a href="about-us.html">Pages</a></li>
                        <li><a href="product.html">About Us</a></li>
                        <li><a href="contact-us.html">Products</a></li>
                        <li><a href="sign-in.html">Portfolio</a></li>
                        <li><a href="sign-in.html">Blog</a></li>
                        <li><a href="sign-in.html">Contact</a></li>
                        <li><a href=""><span class="glyphicon glyphicon-user" aria-hidden="true"></span></a></li>
                        <li><a href=""><span class="glyphicon glyphicon-search" aria-hidden="true"></span></a></li>
                    </ul>
                    <!-- /.nav -->
                </div>
                <!-- /.navbar-collapse -->
            </div>
            <!-- /.container -->
        </nav>
    </header>

    <div class="">
        @RenderBody()
        <hr />
        
    </div>
    <footer class="footer">
        <div class="container">
            <div class="col-sm-6 col-md-7 col-lg-7">
                <p class="text-muted footer-text">OneKonnect Communication simplified and unfied &#64;2015. All Rights Reserved</p>
            </div>
            <div class="col-sm-6 col-md-5 col-lg-5 pull-right">
                <ul class="footer-nav">
                    <li><a href="#">Home</a></li>
                    <li><a href="product.html">About Us</a></li>
                    <li><a href="contact-us.html">Products</a></li>
                    <li><a href="sign-in.html">Contact</a></li>
                </ul>
            </div>
        </div>
    </footer>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/npm.js"></script>
</body>
</html>