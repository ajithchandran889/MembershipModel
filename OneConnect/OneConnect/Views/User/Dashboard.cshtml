                                        <tbody>
                                            @foreach (var item in Model.userList) 
                                            {
                                                <tr>
                                                    <td scope="row"><label>@item.customUserId</label></td>
                                                    @*<td>Username 01</td>*@
                                                    <td>
                                                        <label id="@item.userId">@item.emailId</label>
                                                        <input id="email_@(item.userId)" type="text" class="hideElement" value="@item.emailId"/>
                                                    </td>
                                                    @*<td>Full Address, 1270</td>
                                                    <td>onekonnect</td>*@
                                                    <td>
                                                        @*<span>
                                                            <img src="~/Content/site/status-uncheck.png">
                                                        </span>*@
                                                        <input id="checkboxid" type="checkbox" class="checkboxid css-checkbox" userid="@item.userId" @if (item.status) { <text> checked="checked" </text> }>
                                                    </td>
                                                    @*<td>
                                                        <input id="checkboxid" type="checkbox" class="css-checkbox">
                                                    </td>*@
                                                    <td>
                                                        <span class="edit">
                                                            <a href="" id="editUser_@(item.userId)" class="editUser" userid="@item.userId"><img src="~/Content/site/edit.png"></a>
                                                            <a href="" id="saveChanges_@(item.userId)" class="hideElement saveChanges" userid="@item.userId"><img src="~/Content/site/save.png"></a>
                                                        </span>
                                                        <span class="delete">
                                                            <a href="##"><img src="~/Content/site/delete.png"></a>
                                                        </span>
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                                <div class="row edit-fields">
                                    <span> <img src="~/Content/site/star.png" alt="" /></span>
                                    <span>: Editable Fields</span>
                                </div>

                            </div>
                        </div>
                        <div class="tab-pane" id="bb">
                            <div class="row">
                                <div class="bs-example col-md-7">
                                    <form class="form-horizontal">
                                        <div class="form-group">
                                            <label for="inputEmail" class="control-label col-xs-4 col-md-3 col-sm-3">User Email:</label>
                                            <div class="col-xs-8 col-md-9 col-sm-9">
                                                <input type="email" class="form-control" id="inputEmail" placeholder="" autocomplete="off">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="inputConfirmEmail" class="control-label col-xs-4 col-md-3 col-sm-3">Confirm Email:</label>
                                            <div class="col-xs-8 col-md-9 col-sm-9">
                                                <input type="email" class="form-control" id="inputConfirmEmail" placeholder="" autocomplete="off">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="inputPassword" class="control-label col-xs-4 col-md-3 col-sm-3">Password:</label>
                                            <div class="col-xs-8 col-md-9 col-sm-9">
                                                <input type="password" class="form-control" id="inputPassword" placeholder="" autocomplete="off">
                                            </div>
                                        </div>
                                        <div class="form-group account-settings-btn">
                                            <div class="col-xs-12 col-sm-5 col-md-4 col-md-offset-1">
                                                <a class="btn btn-info addNewUser">
                                                    <img class="addNewUser" src="~/Content/site/save.png" alt="">Save Your Changes
